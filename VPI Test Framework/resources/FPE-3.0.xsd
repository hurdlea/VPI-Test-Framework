<xsd:schema xmlns="http://foxtel.tv/engineering/vpi/schemas/exchange/3/0" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://foxtel.tv/engineering/vpi/schemas/exchange/3/0" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!-- ================================================== -->
	<!-- =====  Element Declarations  -->
	<!-- ================================================== -->
	<xsd:element name="FeatureExport" type="FeatureExportType">
		<xsd:annotation>
			<xsd:documentation>Export a customer ready offer package to a receiving system</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="FeatureExportResponse" type="FeatureExportResponseType">
		<xsd:annotation>
			<xsd:documentation>Response message to a feature export from receiving system</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AssetExport" type="AssetExportType">
		<xsd:annotation>
			<xsd:documentation> Publish stand alone video assets to a recieving system</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="AssetLicenseExport" type="AssetLicenseExportType">
		<xsd:annotation>
			<xsd:documentation>Notify the receiving system of a change to the video asset license window</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MediaPurgeRequest" type="FeatureMediaPurgeType">
		<xsd:annotation>
			<xsd:documentation>Purge a video asset from a receiving system</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="MediaPurgeResponse" type="FeatureExportResponseType">
		<xsd:annotation>
			<xsd:documentation>Response message from receiving system to a video purge</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ImagePurgeRequest" type="FeatureImagePurgeType">
		<xsd:annotation>
			<xsd:documentation>Purge an image asset from the receiving system</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="ImagePurgeResponse" type="FeatureExportResponseType">
		<xsd:annotation>
			<xsd:documentation>Response message from receiving system to an image purge</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
