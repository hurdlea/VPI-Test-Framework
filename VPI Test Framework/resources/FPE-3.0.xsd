	<!--  ImageClassificationType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="ImageClassificationType">
		<xsd:annotation>
			<xsd:documentation>List of acceptable values for categorising images [Ref: ...]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="primaryImage"/>
			<xsd:enumeration value="thumbnailImage"/>
			<xsd:enumeration value="optionalOther"/>
			<xsd:enumeration value="frameGrab"/>
			<xsd:enumeration value="userStill"/>
			<xsd:enumeration value="poster"/>
			<xsd:enumeration value="landscape"/>
			<xsd:enumeration value="square"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ImageMediaFormatType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="ImageMediaFormatType">
		<xsd:annotation>
			<xsd:documentation>List of acceptable values for image file formats [Ref: ...]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="JPG"/>
			<xsd:enumeration value="GIF"/>
			<xsd:enumeration value="PNG"/>
			<xsd:enumeration value="TGA"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  LanguageListType  <<list>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="LanguageListType">
		<xsd:annotation>
			<xsd:documentation>Allows a list of language codes</xsd:documentation>
		</xsd:annotation>
		<xsd:list itemType="xsd:language"/>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  OfferType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="OfferType">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="Subscription"/>
			<xsd:pattern value="Subscription\-\S*"/>
			<xsd:pattern value="PPV"/>
			<xsd:pattern value="PPV\-\S*"/>
			<xsd:pattern value="EST"/>
			<xsd:pattern value="EST\-\S*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  OriginatingSystemIdentifierType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="OriginatingSystemIdentifierType">
		<xsd:annotation>
			<xsd:documentation>List of source ids used to identify the source of a package</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Paradigm"/>
			<xsd:enumeration value="FIL"/>
			<xsd:enumeration value="EMAM"/>
			<xsd:enumeration value="IBMS"/>
			<xsd:enumeration value="Dayport"/>
			<xsd:enumeration value="HWW"/>
			<xsd:enumeration value="Redbee">
				<xsd:annotation>
					<xsd:documentation>Redbee Media - Reverse EPG, dirty VOP, and clean VOD content</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ParentalRatingType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="ParentalRatingType">
		<xsd:annotation>
			<xsd:documentation>List of acceptable values for parental rating. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="P"/>
			<xsd:enumeration value="C"/>
			<xsd:enumeration value="G"/>
			<xsd:enumeration value="PG"/>
			<xsd:enumeration value="M"/>
			<xsd:enumeration value="MA15+"/>
			<xsd:enumeration value="AV"/>
			<xsd:enumeration value="R18+"/>
			<xsd:enumeration value="NC"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- PlatformType-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="PlatformType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="PC"/>
			<xsd:enumeration value="XBOX"/>
			<xsd:enumeration value="IPJ"/>
			<xsd:enumeration value="HD_VOD"/>
			<xsd:enumeration value="SD_VOD"/>
			<xsd:enumeration value="REPG">
				<xsd:annotation>
					<xsd:documentation>Reverse EPG</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  RegionCodeType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="RegionCodeType">
		<xsd:annotation>
			<xsd:documentation>ISO 3166-2:2007 - Country and region </xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:whiteSpace value="collapse"/>
			<xsd:pattern value="[a-zA-Z]{2}(-[a-zA-Z0-9]{1,3})?"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  RoleType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="RoleType">
		<xsd:annotation>
			<xsd:documentation>List of acceptable values for the role a person performed in a feature. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Actor"/>
			<xsd:enumeration value="Producer"/>
			<xsd:enumeration value="Director"/>
			<xsd:enumeration value="Writer"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  TapeClassificationType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="TapeClassificationType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="feature"/>
			<xsd:enumeration value="promo"/>
			<xsd:enumeration value="trailer"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  TargetDeviceType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="TargetDeviceType">
		<xsd:annotation>
			<xsd:documentation>A list of acceptable values for playback devices. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.38</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="DownloadManager"/>
			<xsd:enumeration value="EmbeddedPlayer"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  TermDetailType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="TermDetailType">
		<xsd:annotation>
			<xsd:documentation>Encapsulates any parameters used for content protection (e.g. Entriq's CPS)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Parameter" type="ContentProtectionParamType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A content protection parameter.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="type" type="xsd:string" use="required"/>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  TermTypeList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="TermListType">
		<xsd:annotation>
			<xsd:documentation>A list of right Terms</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Term" type="TermDetailType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A rights term defining access rights for DRM, rights to media and rights related comments, </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  TimeCodeType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="TimeCodeType">
		<xsd:annotation>
			<xsd:documentation>Timecode hh:mm:ss.ff - where ff is frames and frames are optional</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d\d:\d\d:\d\d(\.\d\d)*"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  VideoClassificationType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="VideoClassificationType">
		<xsd:annotation>
			<xsd:documentation>List of acceptable values for categorising a video asset [Ref: ...]</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="feature"/>
			<xsd:enumeration value="promo"/>
			<xsd:enumeration value="trailer"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  YearDateType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="YearDateType">
		<xsd:annotation>
			<xsd:documentation>A datatype to represent a specific year. The lexical representation is YYYY</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="\d\d\d\d"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PromoCategoryType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="PromoCategoryType">
		<xsd:annotation>
			<xsd:documentation>Promo category enumeration type</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Episodic-A"/>
			<xsd:enumeration value="Episodic-B"/>
			<xsd:enumeration value="SeasonGeneric"/>
			<xsd:enumeration value="ShowGeneric"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Prioity  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="priorityType" id="positiveInteger">
		<xsd:restriction base="xsd:nonNegativeInteger">
			<xsd:minInclusive value="1"/>
			<xsd:maxInclusive value="5"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ================================================== -->
	<!-- =====  Complex Type Definitions  -->
	<!-- ================================================== -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  AwardList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AwardListType">
		<xsd:annotation>
			<xsd:documentation>A list of awards</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Award" type="AwardType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Details of an award. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.38</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Award  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AwardType">
		<xsd:annotation>
			<xsd:documentation>Contains information about an award that was given. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.54</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Year" type="xsd:gYear">
				<xsd:annotation>
					<xsd:documentation>The year the award was given in. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.54</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Category" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The category in which the award was given. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.54</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Result" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The result (e.g. nominee). See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.54</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Recipient" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The recipient of the award. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.54</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="name" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>The name of the award. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.54</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  BinaryMediaItem  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="BinaryMediaItemType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>A binary media item</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MediaItemType">
				<xsd:attribute name="filename" use="required">
					<xsd:annotation>
						<xsd:documentation>Media name</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="contentType" type="ContentTypeType" use="required">
					<xsd:annotation>
						<xsd:documentation>MIME content type</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="fileSize" type="xsd:nonNegativeInteger">
					<xsd:annotation>
						<xsd:documentation>The size of the file in bytes</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="scope" use="optional" default="programme">
					<xsd:annotation>
						<xsd:documentation>Define the scope of the item for episodic content where there is a defined heirachy.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="programme"/>
							<xsd:enumeration value="episode"/>
							<xsd:enumeration value="season"/>
							<xsd:enumeration value="show"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="signature" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>A file signature, such as an MD5 hash. Intended to be used by the receiving end to detect changes in the binary file</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  HostedMediaItem  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="HostedMediaItemType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>A media item that is accessed via another origin location</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MediaItemType">
				<xsd:attribute name="uri" use="required">
					<xsd:annotation>
						<xsd:documentation>Media name</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="contentType" type="ContentTypeType" use="required">
					<xsd:annotation>
						<xsd:documentation>MIME content type</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="nominalSize" type="xsd:nonNegativeInteger" use="required">
					<xsd:annotation>
						<xsd:documentation>The nomial size of the asset in bytes for display to the customer. For streaming this is the largest rendition size.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="resolution" type="VideoFormatType" use="required">
					<xsd:annotation>
						<xsd:documentation>Indicative resolution of the video presentation - used to differentiate multiple transcoded versions of the asset.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="scope" use="optional" default="programme">
					<xsd:annotation>
						<xsd:documentation>Define the scope of the item for episodic content where there is a defined heirachy.</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="programme"/>
							<xsd:enumeration value="episode"/>
							<xsd:enumeration value="season"/>
							<xsd:enumeration value="show"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="sound" type="AvailableAudioType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identify the audio type that is available in the transcoded version.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ContentDescription  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ContentDescriptionType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Encapsulates descriptive information about the package</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="EPGDescriptionType">
				<xsd:sequence>
					<xsd:element name="NominalDuration" type="xsd:duration" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>The nominal duration of the feature. For eample: 120 Mins, or 2 hrs</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ContentProtectionParam  <<complexType>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ContentProtectionParamType">
		<xsd:annotation>
			<xsd:documentation>Defines a content protection parameter</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="name" use="required">
					<xsd:annotation>
						<xsd:documentation>The parameter name</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="ContentProtectionParamNameType"/>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ContentProtection  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ContentProtectionType">
		<xsd:annotation>
			<xsd:documentation>Encapsulates any parameters used for content protection (e.g. Entriq's CPS)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Parameter" type="ContentProtectionParamType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A content protection parameter.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Content  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ContentType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Defines a package content type</xsd:documentation>
		</xsd:annotation>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CountryOfProduction  <<complexType>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CountryOfProductionType">
		<xsd:annotation>
			<xsd:documentation>The country of production, when production began, and when production finished</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="RegionCodeType">
				<xsd:attribute name="from" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>The date when production began in the specified country</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="to" type="xsd:date">
					<xsd:annotation>
						<xsd:documentation>The date when production finished in the specified country.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  CreditList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="CreditListType">
		<xsd:annotation>
			<xsd:documentation>A list of credits</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Credit" type="FeatureCreditType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>One or more credits. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DateTimeWindow  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DateTimeWindowPeriodType">
		<xsd:annotation>
			<xsd:documentation>Used to specify a window of time</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="start" type="xsd:dateTime" use="required">
			<xsd:annotation>
				<xsd:documentation>The exact starting date and time.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="end" type="xsd:dateTime" use="required">
			<xsd:annotation>
				<xsd:documentation>The exact ending date and time.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DateTimeWindow  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DateTimeWindowType">
		<xsd:annotation>
			<xsd:documentation>Used to specify a window of time</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="start" type="xsd:dateTime" use="required">
			<xsd:annotation>
				<xsd:documentation>The exact starting date and time.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="end" type="xsd:dateTime" use="required">
			<xsd:annotation>
				<xsd:documentation>The exact ending date and time.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="offerType" type="OfferType" use="optional" default="Subscription">
			<xsd:annotation>
				<xsd:documentation>Indicates if this offer is PPV or Subscription</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="price" default="0.00">
			<xsd:annotation>
				<xsd:documentation>Price in 0.00 format required when the offerType is PPV</xsd:documentation>
			</xsd:annotation>
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:pattern value="(\d+\.\d\d)"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DestinationDetail  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DestinationDetailType">
		<xsd:annotation>
			<xsd:documentation>Used to define destination and optional destination parameters</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Key" type="KeyValueType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A list of keys and values that could be used to convey destination specific parameters or metadata.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="destinationID" type="DestinationType" use="required">
			<xsd:annotation>
				<xsd:documentation>Specifies the id used to determine the intended destination for a package.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="deviceId" type="DeviceType" use="optional">
			<xsd:annotation>
				<xsd:documentation>Specifies the id used to determine the intended device.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="accessTerm" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>Specifies the Term id used for Access Control.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="productTerm" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>Specifies the Term id used to list the Product Propositions.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  DestinationList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DestinationListType">
		<xsd:annotation>
			<xsd:documentation>A list of destination</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Destination" type="DestinationDetailType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A destination for which the content description and associated media is to be distrbuted. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.6</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="destinationID" type="DestinationType" use="optional">
			<xsd:annotation>
				<xsd:documentation>Specifies the default id used to determine the intended destination for a package.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="accessTerm" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>Specifies the default Term id for Access Control. Will typically also be used where the Platform does not support individual device control</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="productTerm" type="xsd:string" use="optional">
			<xsd:annotation>
				<xsd:documentation>Specifies the default Term id that lists the Product Propositions. Will typically also be used where the Platform does not support individual Product Propositions.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- DisplayMessageType-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="DisplayMessageType">
		<xsd:annotation>
			<xsd:documentation>Used to pass display message</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="xsd:anyType"/>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- EPGLanguageListType-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="EPGLanguageType">
		<xsd:complexContent>
			<xsd:extension base="EPGDescriptionType">
				<xsd:attribute name="name" type="xsd:language" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- EPGPlatformListType-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="EPGPlatformType">
		<xsd:complexContent>
			<xsd:extension base="EPGDescriptionType">
				<xsd:attribute name="name" type="PlatformType" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- EPGLanguageListType-->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="EPGDescriptionListType">
		<xsd:annotation>
			<xsd:documentation>Defines an EPG description for a language</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="Language" type="EPGLanguageType"/>
			<xsd:element name="Platform" type="EPGPlatformType"/>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  EPGDescriptionType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="EPGDescriptionType">
		<xsd:annotation>
			<xsd:documentation>Defines an EPG description type</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Title" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>A title. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1.9</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SortTitle" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Title that provides A-Z sorting hints i.e. Simpsons, The</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EventTitle" type="xsd:string" nillable="true" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The title used for all display points. It allows for deviations from program title such as mapping, or channel preferred presentation style.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ShortSynopsis" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>A short synopsis. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1.25</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LongSynopsis" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A longer synopsis.See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1.24</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DisplaySeriesEpNum" type="xsd:boolean" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ExternalRefList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ExternalRefListType">
		<xsd:annotation>
			<xsd:documentation>A list of External Web and other sites.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ExternalRef" type="ExternalRefType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ExternalRef  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ExternalRefType">
		<xsd:sequence>
			<xsd:element name="TitleId" type="xsd:string"/>
			<xsd:element name="SeriesId" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="site" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>The site Key. May be the site address or a code. The downstram system converts this referece into a URL by way of a lookup from its own tables so that the address my change but the reference stays the same. </xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureContentExport  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ContentIdentifierType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="context" use="required">
					<xsd:annotation>
						<xsd:documentation>Specifies the context; e.g. Asset, Season, Show, Group or implied by the format attribute</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="asset"/>
							<xsd:enumeration value="season"/>
							<xsd:enumeration value="show"/>
							<xsd:enumeration value="group"/>
							<xsd:enumeration value="offer"/>
							<xsd:enumeration value="event"/>
							<xsd:enumeration value="programme"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="provider" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>The id format; e.g. IMDB, TMS, EIDR, UMID, ISAN, ...</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureContentExport  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureContentExportType">
		<xsd:sequence>
			<xsd:element name="Description" type="FeatureDescriptionType"/>
			<xsd:element name="ContentIdentification" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="ContentIdentifier" type="ContentIdentifierType" maxOccurs="unbounded"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="Images" type="ImageListType"/>
			<xsd:element name="Tags" type="TagListType" minOccurs="0"/>
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>Identifer for the feature object</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureContentImport  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureContentImportType">
		<xsd:sequence>
			<xsd:element name="Protection" type="ContentProtectionType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Parameters for content protect, i.e. DRM</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Description" type="FeatureImportDescriptionType">
				<xsd:annotation>
					<xsd:documentation>Contains detail about the feature.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Tags" type="TagListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A container for additional metadata items. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.57</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureCredit  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureCreditType">
		<xsd:annotation>
			<xsd:documentation>Used to credit people for their role in a production. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Person" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The name of the person that performed a role in the feature. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Character" type="xsd:string" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The name of the character. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="role" type="RoleType" use="required">
			<xsd:annotation>
				<xsd:documentation>The role a person performed in a feature. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  SimilarTitleListType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="SimilarTitleListType">
		<xsd:annotation>
			<xsd:documentation>Specifies a list of similar titles (groupIds)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="GroupId" type="xsd:string" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MappedTitleType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MappedTitleType">
		<xsd:annotation>
			<xsd:documentation>Specifies a mapped title title</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="type" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>The mapped title type e.g. EPG or IPJ versions</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MappedTitleListType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MappedTitleListType">
		<xsd:annotation>
			<xsd:documentation>Specifies a list of mapped titles</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Title" type="MappedTitleType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureImportDescription  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureImportDescriptionType">
		<xsd:annotation>
			<xsd:documentation>Encapsulates descriptive information about a feature import</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="FeatureDescriptionType">
				<xsd:sequence>
					<xsd:element name="SeriesDescription" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Series or group description for boxed sets and season purchases</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="MappedTitles" type="MappedTitleListType" minOccurs="0" maxOccurs="unbounded">
						<xsd:annotation>
							<xsd:documentation>A title field supplied in addition to the official me title that is the EPG title</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SimilarTitles" type="SimilarTitleListType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureDescription  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureDescriptionType">
		<xsd:annotation>
			<xsd:documentation>Encapsulates descriptive information about a feature</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="ContentDescriptionType">
				<xsd:sequence>
					<xsd:element name="Genre" type="GenreType">
						<xsd:annotation>
							<xsd:documentation>Specifies the genre. See document FD-T-E-2590 Issue 1, Rev 2 section  3.1.1.15</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Production" type="FeatureProductionType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Contains additional detail about the production. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Atmosphere" type="AtmosphereListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Used to describe the Atmosphere. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Reviews" type="ReviewListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>A list of reviews. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1.29</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Awards" type="AwardListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>A list of awards given. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.54</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Ratings" type="RatingListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>A list of ratings.a score out of n</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="SeasonNumber" type="xsd:positiveInteger" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>FD-T-E-2590 2.1.1.11</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EpisodeNumber" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>FD-T-E-2590 2.1.1.13</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="EpisodeTitle" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>FD-T-E-2590 2.1.1.14</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="titleId" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>Used to group multiple season's episodic titles together. ie The Simpsons</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="category" type="FeatureCategoryType" use="required">
					<xsd:annotation>
						<xsd:documentation>The category that this package belongs to. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.7</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="library" type="xsd:boolean" default="false">
					<xsd:annotation>
						<xsd:documentation>Identifies content as being part of a long term library of material. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.7a</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="programmeId" use="required">
					<xsd:annotation>
						<xsd:documentation>To be used by downstream EPG. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.10</xsd:documentation>
					</xsd:annotation>
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"/>
							<xsd:pattern value="\w+"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureExport  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureExportType">
		<xsd:complexContent>
			<xsd:extension base="PackageExportType">
				<xsd:sequence>
					<xsd:element name="Offer" type="PackageOfferType"/>
					<xsd:element name="Feature" type="FeatureContentExportType" minOccurs="0"/>
					<xsd:element name="Media" type="MediaExportType" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MediaExport  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AssetExportType">
		<xsd:complexContent>
			<xsd:extension base="MessageType">
				<xsd:sequence>
					<xsd:element name="Header" type="PackageHeaderType"/>
					<xsd:element name="Videos" type="VideoListType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MediaLicenseExportType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="AssetLicenseExportType">
		<xsd:annotation>
			<xsd:documentation>Defines a license change for an asset export to receiving systems</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MessageType">
				<xsd:sequence>
					<xsd:element name="AssetId" type="xsd:string" nillable="false"/>
					<xsd:element name="LicenseWindow" type="DateTimeWindowType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureMediaPurgeType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureMediaPurgeType">
		<xsd:annotation>
			<xsd:documentation>Defines a media purge message to receiving systems</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MessageType">
				<xsd:sequence>
					<xsd:element name="AssetId" type="xsd:string" nillable="false"/>
					<xsd:element name="RequestId" type="xsd:string"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureImagePurgeType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureImagePurgeType">
		<xsd:annotation>
			<xsd:documentation>Defines a media purge message to receiving systems</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MessageType">
				<xsd:sequence>
					<xsd:element name="ImageId" type="xsd:string" nillable="false"/>
					<xsd:element name="RequestId" type="xsd:string"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureMediaPurgeType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureExportResponseType">
		<xsd:annotation>
			<xsd:documentation>Defines a response message from the receiving system</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MessageType">
				<xsd:sequence>
					<xsd:element name="RequestId" type="xsd:string" minOccurs="0"/>
					<xsd:choice>
						<xsd:element name="PackageId"/>
						<xsd:element name="AssetId" type="xsd:string" nillable="false"/>
						<xsd:element name="ImageId" type="xsd:string" nillable="false"/>
					</xsd:choice>
					<xsd:element name="Status">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="SUCCESS"/>
								<xsd:enumeration value="INGESTING"/>
								<xsd:enumeration value="PUBLISHING"/>
								<xsd:enumeration value="FAIL"/>
								<xsd:enumeration value="HALTED"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="Message" type="xsd:string" minOccurs="0"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureImport  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureImportType">
		<xsd:annotation>
			<xsd:documentation>Defines a feature specific package</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="PackageImportType">
				<xsd:sequence>
					<xsd:element name="Header" type="PackageHeaderType">
						<xsd:annotation>
							<xsd:documentation>A Container for package header information.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Feature" type="FeatureContentImportType">
						<xsd:annotation>
							<xsd:documentation>A container for the package content description.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Media" type="MediaImportType">
						<xsd:annotation>
							<xsd:documentation>A container for the package media detail.</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureProduction  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureProductionType">
		<xsd:annotation>
			<xsd:documentation>Defines feature specific production information</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Year" type="YearDateType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The year of production and/or release. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1.28</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Colour" type="ColourType" default="Colour" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The type of colour. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1.34</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Languages" type="LanguageListType" default="en" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A list of spoken languages. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1.27</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Country" type="CountryOfProductionType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A list of countries where production took place. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1.23</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Credits" type="CreditListType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A list of credits. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FeatureReview  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="FeatureReviewType">
		<xsd:annotation>
			<xsd:documentation>Defines a feature review element type. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Reviewer" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The name of the person who did the review.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Source" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The source from which the review was obtained.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Text" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>The text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="date" type="xsd:date">
			<xsd:annotation>
				<xsd:documentation>The date of the review</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ImageFile  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ImageFileType">
		<xsd:complexContent>
			<xsd:extension base="BinaryMediaItemType">
				<xsd:attribute name="class" type="ImageClassificationType" use="required">
					<xsd:annotation>
						<xsd:documentation>Classification of the image, e.g. it's a thumbnail.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="width" type="xsd:positiveInteger">
					<xsd:annotation>
						<xsd:documentation>The width of the image</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="height" type="xsd:positiveInteger">
					<xsd:annotation>
						<xsd:documentation>The height of the image</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="imageId" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Unique id of the image that is consistant across multiple offers for the same media asset.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ImageList  <<complexType, choice>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ImageListType">
		<xsd:annotation>
			<xsd:documentation>A list of images</xsd:documentation>
		</xsd:annotation>
		<xsd:choice maxOccurs="unbounded">
			<xsd:element name="ImageSource" type="ImageSourceType">
				<xsd:annotation>
					<xsd:documentation>A image source</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ImageMediaContent  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ImageMediaContentType">
		<xsd:complexContent>
			<xsd:extension base="ImageFileType">
				<xsd:sequence>
					<xsd:element name="Contents" type="xsd:base64Binary">
						<xsd:annotation>
							<xsd:documentation>The base64 encoded binary image data</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ImageSource  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ImageSourceType">
		<xsd:sequence>
			<xsd:element name="Image" type="ImageFileType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>One or more images</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  KeyValue  <<complexType>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="KeyValueType">
		<xsd:annotation>
			<xsd:documentation>Defines a key/value pair</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="name" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>The key name or id.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MediaExport  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MediaExportType">
		<xsd:complexContent>
			<xsd:extension base="MediaType">
				<xsd:sequence>
					<xsd:element name="Videos" type="VideoListType"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MediaImport  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MediaImportType">
		<xsd:annotation>
			<xsd:documentation>Defines the packages' source media</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MediaType">
				<xsd:sequence>
					<xsd:element name="Images" type="ImageListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Details of supplied images</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:choice>
						<xsd:element name="Tapes" type="TapeListType">
							<xsd:annotation>
								<xsd:documentation>Video media from one or more video tapes</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="Videos" type="VideoImportListType"/>
					</xsd:choice>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MediaItemSource  <<complexType>>  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MediaItemSourceType">
		<xsd:simpleContent>
			<xsd:extension base="xsd:anyURI">
				<xsd:attribute name="contentType" type="ContentTypeType" use="required"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  MediaItem  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MediaItemType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>An abstract media item type.</xsd:documentation>
		</xsd:annotation>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Media  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MediaType"/>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PackageExport  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PackageExportType">
		<xsd:complexContent>
			<xsd:extension base="PackageType"/>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PackageHeader  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PackageHeaderType">
		<xsd:annotation>
			<xsd:documentation>Defines a package header type</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ExhibitionWindow" type="DateTimeWindowType">
				<xsd:annotation>
					<xsd:documentation>The period for which the export is available to the customer. For a FeatureExport this is the offer duration and for AssetExport it represents the asset availability period before purge. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.5</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Terms" type="TermListType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The rights terms associated with a programme.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Destinations" type="DestinationListType">
				<xsd:annotation>
					<xsd:documentation>Contains a list of destinations to which this package will be distributed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  OfferCategoryListType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="OfferCategoryType">
		<xsd:annotation>
			<xsd:documentation>Defines the editorial categories for an offer. This does not affect or replace any automated categorisation processes on the video platform.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Category">
				<xsd:complexType>
					<xsd:attribute name="start" type="xsd:dateTime" use="optional">
						<xsd:annotation>
							<xsd:documentation>When the offer is available in the category. If ommited the offer is available immediately on ExhibitionWindow start.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="end" type="xsd:dateTime" use="optional">
						<xsd:annotation>
							<xsd:documentation>When the offer is removed from the category (cannot be greater than ExhibitionWindow end.) If ommited the offer is removed on ExhibitionWindow start.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
					<xsd:attribute name="path" type="xsd:string" use="required">
						<xsd:annotation>
							<xsd:documentation>The editorial categorisation path to diplay the offer.</xsd:documentation>
						</xsd:annotation>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  OfferPresentationType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="OfferPresentationType">
		<xsd:annotation>
			<xsd:documentation>Defines the presentation of an offer on a distribution headend</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="DisplayAsNew" type="xsd:duration" minOccurs="0"/>
			<xsd:element name="DisplayAsLastChance" type="xsd:duration" minOccurs="0"/>
			<xsd:element name="Categories" type="OfferCategoryType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  OfferPresentationType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="OfferBundleType">
		<xsd:annotation>
			<xsd:documentation>Defines the titles associated with this offer</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ContentGroup" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="FeatureRef">
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="xsd:string">
										<xsd:attribute name="id" use="required">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:attribute>
									</xsd:extension>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="VideoRef" maxOccurs="10">
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="xsd:string">
										<xsd:attribute name="id" use="required">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:attribute>
										<xsd:attribute name="class" type="VideoClassificationType" use="required"/>
									</xsd:extension>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="ImageRef" maxOccurs="10">
							<xsd:complexType>
								<xsd:simpleContent>
									<xsd:extension base="xsd:string">
										<xsd:attribute name="id" use="required">
											<xsd:simpleType>
												<xsd:restriction base="xsd:string">
													<xsd:minLength value="1"/>
												</xsd:restriction>
											</xsd:simpleType>
										</xsd:attribute>
										<xsd:attribute name="class" type="ImageClassificationType" use="required"/>
										<xsd:attribute name="format" type="ImageMediaFormatType"/>
									</xsd:extension>
								</xsd:simpleContent>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
					<xsd:attribute name="name" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
					<xsd:attribute name="offerRef" use="optional">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="1"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ConsumerList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ConsumerWarningList">
		<xsd:sequence>
			<xsd:element name="Warning" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:attribute name="order" type="xsd:positiveInteger" use="required"/>
					<xsd:attribute name="severity" type="xsd:positiveInteger" use="required"/>
					<xsd:attribute name="label" type="xsd:string" use="required"/>
					<xsd:attribute name="code" type="ConsumerWarningType" use="required"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PackageOffer  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PackageOfferType">
		<xsd:annotation>
			<xsd:documentation>Defines a package header type</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ExhibitionWindow" type="DateTimeWindowType">
				<xsd:annotation>
					<xsd:documentation>The period for which the export is available to the customer. For a FeatureExport this is the offer duration and for AssetExport it represents the asset availability period before purge. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.5</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ProviderId" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Provide downstream systems with the means to identify the content owner or provider. For example; a channel is a provider. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.4</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Distributor" minOccurs="0">
			<xsd:complexType>
				<xsd:attribute name="reportingCode" use="required">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="name" use="required">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:minLength value="1"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:complexType>
			</xsd:element>
			<xsd:element name="Deal" minOccurs="0">
			<xsd:complexType>
				<xsd:attribute name="purchaseId" type="xsd:string" use="required"/>
				<xsd:attribute name="groupId" type="xsd:string" use="required"/>
			</xsd:complexType>
			</xsd:element>
			<xsd:element name="DisplayMessages" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="DisplayMessage" type="DisplayMessageType" maxOccurs="99"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="MarketingMessage" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A marketing message. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1.26</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Presentation" type="OfferPresentationType" minOccurs="0"/>
			<xsd:element name="ParentalRating" type="ParentalRatingType"/>
			<xsd:element name="ConsumerWarnings" type="ConsumerWarningList" minOccurs="0"/>
			<xsd:element name="VideoFormat" type="VideoFormatType">
				<xsd:annotation>
					<xsd:documentation>Used to communicate the video format (e.g. SD, HD, 3D)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Sound" type="AvailableAudioType">
				<xsd:annotation>
					<xsd:documentation>Used to communicate the best Sound option</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Widescreen" type="xsd:boolean" default="true" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Used to identify if content is true widescreen or not</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Subtitled" type="xsd:boolean" default="false" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Indicates if video subtitles are present. See document FD-T-E-2590 Issue 1, Rev 2 section 3.1.1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ContentGrouping" type="OfferBundleType">
				<xsd:annotation>
					<xsd:documentation>Used to associate offer criteria to multiple titles for EST and PPV multi-content bundles e.g. Star Wars movies.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Terms" type="TermListType" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The rights terms associated with a programme.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Destinations" type="DestinationListType">
				<xsd:annotation>
					<xsd:documentation>Contains a list of destinations to which this package will be distributed.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="id" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>Identifer for the offer object i.e. the IBMS offerId (was packageId)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="purchaseGroupId" type="xsd:string">
			<xsd:annotation>
				<xsd:documentation>Related to the IBMS deal purchase identifier</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="activate" type="xsd:boolean" default="true">
			<xsd:annotation>
				<xsd:documentation>The package is either active or inactive. Used to indicated to downstream system that it's safe to use the package.</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="operation" type="CommandOperationType" use="required">
			<xsd:annotation>
				<xsd:documentation>Identifer for the offer object i.e. the IBMS offerId (was packageId)</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  PackageImport  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PackageImportType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Defines an import package</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="PackageType"/>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Package  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PackageType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Defines a package</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MessageType">
				<xsd:attribute name="signature" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>A signature that identifies the unique combination of the Offer, Feature and Media objects.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="providerGroup" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Defines the parent entity of the package Content Provider. Eg. for use in defining security adound the package.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Message  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="MessageType" abstract="true">
		<xsd:annotation>
			<xsd:documentation>Defines a message structure</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="schemaVersion" type="xsd:string" use="required" fixed="3.0.0">
			<xsd:annotation>
				<xsd:documentation>The version of schema this package conforms to. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.1</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="origin" type="xsd:string" use="required">
			<xsd:annotation>
				<xsd:documentation>Used to identify the source that generated the package</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="priority" type="priorityType" use="optional">
			<xsd:annotation>
				<xsd:documentation>Used to identify the source that generated the package</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="publishedDate" type="xsd:dateTime" use="required">
			<xsd:annotation>
				<xsd:documentation>The time and date of when an instance document was generated. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.3</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Playback  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="PlaybackType">
		<xsd:sequence>
			<xsd:element name="AccessControl" type="ContentProtectionType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="Devices" type="TargetDeviceListType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  ReviewList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="ReviewListType">
		<xsd:annotation>
			<xsd:documentation>A list of reviews</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Review" type="FeatureReviewType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  RatingList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="RatingListType">
		<xsd:annotation>
			<xsd:documentation>A list of ratings</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Rating" type="RatingType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Rating  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="RatingType">
		<xsd:sequence>
			<xsd:element name="Score" type="xsd:nonNegativeInteger"/>
			<xsd:element name="OutOf" type="xsd:positiveInteger"/>
		</xsd:sequence>
		<xsd:attribute name="type" type="xsd:string" use="required"/>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  TagList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="TagListType">
		<xsd:annotation>
			<xsd:documentation>A list of tags. Tags are key and value pairs</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Tag" type="KeyValueType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A list of arbitrary keys and values that could be used to carry additional metadata specific to the content. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.57</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  TapeList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="TapeListType">
		<xsd:annotation>
			<xsd:documentation>A list of tape reels</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Tape" type="TapeMediaItemType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  TapeMediaItem  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="TapeMediaItemType">
		<xsd:annotation>
			<xsd:documentation>Defines a tape media item</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="MediaItemType">
				<xsd:sequence>
					<xsd:element name="Title" type="xsd:string" minOccurs="0"/>
					<xsd:element name="FormatDescription" type="xsd:string" minOccurs="0"/>
					<xsd:element name="AspectRatio" type="AspectRatioType"/>
					<xsd:element name="ResolutionDescription" type="xsd:string" minOccurs="0"/>
					<xsd:element name="Sound" type="AvailableAudioType" default="Stereo" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifies the available audio type</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ClosedCaptions" type="xsd:boolean" default="false" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates whether closed captioning is carried with the content</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Segments" type="SegmentListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Tape segments</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="libraryId" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>In-house tape library identifier</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="originalId" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Original, or owners' tape identifier</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="reelNumber" type="xsd:positiveInteger" use="required">
					<xsd:annotation>
						<xsd:documentation>Reel number</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="automationId" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>Automation identifier</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="class" type="TapeClassificationType"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  SegmentList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="SegmentListType">
		<xsd:annotation>
			<xsd:documentation>A list of tape segments</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Segment" type="SegmentType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Tape segment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  Segment  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="SegmentType">
		<xsd:annotation>
			<xsd:documentation>A video tape segment</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Title" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Segment title</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Category" type="SegmentCategoryType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Segment Category, ie. Spot, programme</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SOM" type="xsd:duration">
				<xsd:annotation>
					<xsd:documentation>Time point for start of segment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EOM" type="xsd:duration">
				<xsd:annotation>
					<xsd:documentation>Time point for end of segment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="segmentNumber" type="xsd:positiveInteger" use="required">
			<xsd:annotation>
				<xsd:documentation>Segment number</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  TargetDeviceList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="TargetDeviceListType">
		<xsd:sequence>
			<xsd:element name="Device" type="TargetDeviceType" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The intended playback device. See document FD-T-E-2590 (Issue 1, Rev 2), section 3.1.1.38</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  VideoFileList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="VideoAssetListType">
		<xsd:attribute name="base" type="xsd:anyURI" use="required"/>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  VideoFile  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="VideoFileType">
		<xsd:complexContent>
			<xsd:extension base="BinaryMediaItemType">
				<xsd:attribute name="bitrate" type="xsd:nonNegativeInteger" use="required">
					<xsd:annotation>
						<xsd:documentation>Average bitrate in bits per second</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="width" type="xsd:positiveInteger" use="required"/>
				<xsd:attribute name="height" type="xsd:positiveInteger" use="required"/>
				<xsd:attribute name="framerate" type="xsd:positiveInteger" default="25"/>
				<xsd:attribute name="interlaced" type="xsd:boolean" default="false"/>
				<xsd:attribute name="resolution" type="VideoFormatType" use="required">
					<xsd:annotation>
						<xsd:documentation>Indentify the resolution of the transcoded version of the asset.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="sound" type="AvailableAudioType" use="required">
					<xsd:annotation>
						<xsd:documentation>Identify the audio type that is available in the transcoded version.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="assetId" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Unique id of the media that is consistant across multiple offers for the same media asset.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  VideoLocation  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="VideoLocationType">
		<xsd:complexContent>
			<xsd:extension base="HostedMediaItemType">
				<xsd:attribute name="variant" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>Description of the content profile. A typical usage would be to provide low, normal and HD options</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="assetId" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Unique id of the media that is consistant across multiple offers for the same media asset.</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  VideoImportList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="VideoImportListType">
		<xsd:sequence>
			<xsd:element name="Video" type="VideoImportMediaType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  VideoList  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="VideoListType">
		<xsd:sequence>
			<xsd:element name="Video" type="VideoMediaType" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  VideoMedia  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="VideoMediaType">
		<xsd:complexContent>
			<xsd:extension base="MediaItemType">
				<xsd:sequence>
					<xsd:element name="LicenseWindow" type="DateTimeWindowPeriodType" minOccurs="0"/>
					<xsd:element name="MediaDescription" type="xsd:string" minOccurs="0"/>					
					<xsd:element name="ParentalRating" type="ParentalRatingType" minOccurs="0"/>
					<xsd:element name="ConsumerWarnings" type="ConsumerWarningListType" minOccurs="0"/>
					<xsd:element name="Duration" type="xsd:duration"/>
					<xsd:element name="AspectRatio" type="AspectRatioType"/>
					<xsd:element name="ClosedCaptions" type="xsd:boolean" default="false" minOccurs="0"/>
					<xsd:element name="Playback" type="PlaybackType" minOccurs="0"/>
					<xsd:element name="Segments" type="SegmentListType" minOccurs="0"/>
					<xsd:element name="Files">
						<xsd:complexType>
							<xsd:choice maxOccurs="unbounded">
								<xsd:element name="File" type="VideoFileType" maxOccurs="unbounded"/>
								<xsd:element name="Location" type="VideoLocationType" maxOccurs="unbounded"/>
							</xsd:choice>
						</xsd:complexType>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="class" type="VideoClassificationType" use="required"/>
				<xsd:attribute name="versionId" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>Used to identify different cuts of the same feature</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="versionCopyId" type="xsd:string"/>
				<xsd:attribute name="assetId" type="xsd:string" use="required"/>
				<xsd:attribute name="operation" type="CommandOperationType" use="required"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  VideoMedia  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:complexType name="VideoImportMediaType">
		<xsd:complexContent>
			<xsd:extension base="MediaItemType">
				<xsd:sequence>
					<xsd:element name="Title" type="xsd:string" minOccurs="0"/>
					<xsd:element name="AspectRatio" type="AspectRatioType"/>
					<xsd:element name="ResolutionDescription" type="xsd:string" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>HD/SD Description</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Sound" type="AvailableAudioType" default="Stereo" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Identifies the available audio type</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="ClosedCaptions" type="xsd:boolean" default="false" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Indicates whether closed captioning is carried with the content</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
					<xsd:element name="Segments" type="SegmentListType" minOccurs="0">
						<xsd:annotation>
							<xsd:documentation>Tape segments</xsd:documentation>
						</xsd:annotation>
					</xsd:element>
				</xsd:sequence>
				<xsd:attribute name="reelNumber" type="xsd:positiveInteger" use="required">
					<xsd:annotation>
						<xsd:documentation>Reel number</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="filename" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:documentation>Filename of the video asset</xsd:documentation>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="class" type="TapeClassificationType"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!--  FormatType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<xsd:simpleType name="VideoFormatType">
		<xsd:annotation>
			<xsd:documentation>Used to communicate the video format (e.g. SD, HD or 3D)</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="SD">
				<xsd:annotation>
					<xsd:documentation>Standard Definition</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="HD">
				<xsd:annotation>
					<xsd:documentation>High Definition</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="3D">
				<xsd:annotation>
					<xsd:documentation>3D</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="UltraHD">
				<xsd:annotation>
					<xsd:documentation>Ultra High Definition, also called 4K</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
</xsd:schema>
